{
    "rules": {
        "class-name": true,
        "curly": true,
        "eofline": true,
        "forin": true,
        "indent": [
            false,
            "spaces",
            4
        ],
        "label-position": true,
        "max-line-length": [
            true,
            150
        ],
        "no-arg": true,
        "no-bitwise": true,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unused-expression": true,
        "no-use-before-declare": true,
        "no-var-requires": false,
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-whitespace"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "typedef": [
            true,
            "parameter"
        ],
        "variable-name": [
            true,
            "allow-leading-underscore",
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    }
}